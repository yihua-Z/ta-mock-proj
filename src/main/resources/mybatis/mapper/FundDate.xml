<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.psbc.mapper.FundDateMapper">

    <!--    FundDateMapper.xml-->

    <select id="selectByPrimaryKey" resultType="FundDate">
        select *
        from fund_date
        where TACode = #{id}
    </select>


    <delete id="deleteByPrimaryKey">
        delete
        from fund_date
        where TACode = #{id}
    </delete>


    <insert id="insert" parameterType="com.psbc.pojo.FundDate">
        insert into fund_date (TACode,
                               FundCode,
                               DistributorCode,
                               DateType,
                               CorrCfmDate,
                               TransactionCfmDate)
        values (#{TACode},
                #{FundCode},
                #{DistributorCode},
                #{DateType},
                #{CorrCfmDate},
                #{TransactionCfmDate})

    </insert>

    <update id="updateByPrimaryKey" parameterType="com.psbc.pojo.FundDate">
        update fund_date
        set
            FundCode        = #{FundCode},
            DistributorCode = #{DistributorCode},
            DateType        = #{DateType},
            CorrCfmDate     = #{CorrCfmDate}
        where TACode = #{TACode}
    </update>
</mapper>