<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.psbc.mapper.FundDateMapper">

    <!--    FundDateMapper.xml-->

    <select id="selectByPrimaryKey" resultType="FundDate">
        select *
        from fundDate
        where TAAccountID = #{id}
    </select>


    <delete id="deleteByPrimaryKey">
        delete
        from fundDate
        where TACode = #{id}
    </delete>


    <insert id="insert" parameterType="com.psbc.pojo.FundDate">
        insert into mybatis.fundDate (TACode,
                                      FundCode,
                                      DistributorCode,
                                      DateType,
                                      CorrCfmDate,
                                      TransactionCfmDate)
        values (#{TACode},
                #{FundCode},
                #{DistributorCode},
                #{DateType},
                #{CorrCfmDate},
                #{TransactionCfmDate})

    </insert>

    <update id="updateByPrimaryKey" parameterType="com.psbc.pojo.FundDate">
        update fundDate
        set FundCode, = #{FundCode},
            DistributorCode, = #{DistributorCode},
            DateType, = #{DateType},
            CorrCfmDate, = #{CorrCfmDate},
        where TACode = #{TACode}
    </update>
</mapper>